            <Text id="cardtext" fx:id="txtFightStr" strokeType="OUTSIDE" strokeWidth="0.0" text="0" textAlignment="CENTER">
               <font>
                  <Font size="21.0" />
               </font>
               <effect>
                  <Glow />
               </effect>
               <HBox.margin>
                  <Insets left="15.0" />
               </HBox.margin>
            </Text>
         </children>
      </HBox>
      <VBox id="backround" alignment="CENTER" prefHeight="103.0" prefWidth="238.0" style="-fx-background-color: #ffe680;">
         <children>
            <Button id="button" fx:id="confirmBtn" mnemonicParsing="false" prefHeight="26.0" prefWidth="141.0" stylesheets="@styles.css" text="Attack" />
            <Button id="button" fx:id="abortBtn" mnemonicParsing="false" prefHeight="26.0" prefWidth="142.0" stylesheets="@styles.css" text="Abort">
