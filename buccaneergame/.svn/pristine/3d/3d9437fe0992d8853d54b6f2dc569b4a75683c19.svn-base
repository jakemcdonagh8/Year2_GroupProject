<VBox id="backround" alignment="CENTER" prefHeight="440.0" prefWidth="327.0" spacing="5.0" style="-fx-background-color: #ffe680;" stylesheets="@styles.css" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="ui.game.W_FightConfirm">
   <children>
      <HBox id="backround1">
         <children>
            <Text id="backround" strokeType="OUTSIDE" strokeWidth="0.0" text="Do you wish to attack">
