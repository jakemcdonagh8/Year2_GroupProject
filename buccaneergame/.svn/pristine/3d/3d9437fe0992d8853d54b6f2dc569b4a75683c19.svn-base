               <effect>
                  <Glow />
               </effect>
            </Text>
            <HBox id="backround" fx:id="hboxYour" alignment="CENTER" prefHeight="79.0" prefWidth="200.0" style="-fx-background-color: #ffe680;">
               <VBox.margin>
                  <Insets top="15.0" />
               </VBox.margin>
            </HBox>
            <Text id="cardtext" strokeType="OUTSIDE" strokeWidth="0.0" text="Enemy Ship:">
               <font>
                  <Font size="21.0" />
               </font>
               <effect>
                  <Glow />
               </effect>
            </Text>
            <HBox id="backround1" fx:id="hboxEnemy" alignment="CENTER" prefHeight="79.0" prefWidth="200.0" stylesheets="@styles.css">
               <VBox.margin>
                  <Insets top="15.0" />
               </VBox.margin>
            </HBox>
         </children>
      </VBox>
      <HBox id="backround" alignment="CENTER" style="-fx-background-color: #ffe680;">
         <children>
            <Text id="cardtext" strokeType="OUTSIDE" strokeWidth="0.0" text="Your fighting strength:" textAlignment="CENTER">
