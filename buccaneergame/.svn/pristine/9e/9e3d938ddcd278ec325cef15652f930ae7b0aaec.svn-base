package uk.ac.aber.cs211.group07.system.exceptions;

/**
 * Thrown when a request to get a null state (either wrong string or state not initialised)
 * @author Kamil Cupial
 * @version 0.1 - 12/03/17 - initial creation /kac12
 * @version 1.0 - 05/05/17 - Ready for initial release /haa14
 */
@SuppressWarnings("serial")
public class NullStateException extends Exception {
	/**
	 * The exception default constructor
	 */
	public NullStateException(){
		super("The State requested for the given String is empty (null).");
	}
}
